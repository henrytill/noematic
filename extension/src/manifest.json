{
  "manifest_version": 3,
  "name": "Noematic",
  "version": "0.1",
  "description": "Search your backlog",
  "icons": {
    "48": "icons/noematic-48.png"
  },
  "background": {
    "service_worker": "background/background.mjs",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "48": "icons/noematic-48.png"
    },
    "default_popup": "popup/popup.html",
    "default_title": "Noematic"
  },
  "permissions": ["activeTab", "nativeMessaging", "scripting"],
  "key": ""
}
